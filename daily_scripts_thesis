python get_shapes2.py --x-title "#font[12]{p}_{T} [GeV]" --y-title "Probability density" --title "" --norm-binwidth --formula "background_dy_sel,black,2:signal_dy_sel,red" -c mu_selVloose -d Mt_lep_met_c -p tt_lj "signal_dy_sel":distrs/distrs_94v5_processing1_plot2.root:dy_mutau/dy_tautau/NOMINAL/dy_mutau_dy_tautau_NOMINAL_leading_lep_pt "background_dy_sel":distrs/distrs_94v5_processing1_plot2.root:dy_mutau/wjets_other/NOMINAL/dy_mutau_wjets_other_NOMINAL_leading_lep_pt --output-type pdf --no-std-path --fonts1 133 --fonts1-size 30 --norm-formulas --left-title "CMS #font[52]{Internal Simulation}"  --x-range 0,150 --y-range 0,0.5

--draw-overflows 10
--left-title-inside

mv ./compare_mu_selVloose_tt_lj_NOMINAL_Mt_lep_met_c_signal,background_formula.pdf               ./lep_pt_distrs_dyjets.pdf
